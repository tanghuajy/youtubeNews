{
    "manifest_version": 3,
    "name": "YouTube热点新闻监控",
    "version": "1.0",
    "description": "监控YouTube上特定行业的热点新闻",
    "permissions": ["storage", "activeTab", "scripting"],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "popup.html"
    }
}    